{
    "extends": [
      "eslint:recommended",
      "airbnb",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": ["react", "react-hooks", "prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    }
  }